<script>
  const overlay = document.getElementById("overlay");
  const closeBtn = document.getElementById("close");

  // Show overlay function
  function showOverlay() {
    overlay.style.display = "flex";
    closeBtn.classList.add("visible");
  }

  // Hide overlay function
  function hideOverlay() {
    overlay.style.display = "none";
    closeBtn.classList.remove("visible");
  }

  // Card click
  document.getElementById("card").onclick = showOverlay;

  // About button click
  document.getElementById("about").onclick = showOverlay;

  // Close button click
  closeBtn.onclick = function(event) {
    event.stopPropagation();
    hideOverlay();
  };

  // Close overlay when clicking outside
  overlay.onclick = function(event) {
    if (event.target === this) {
      hideOverlay();
    }
  };

  // Hamburger menu toggle
  document.getElementById('hamburger-toggle').addEventListener('change', function() {
    const dashboardContent = document.getElementById('dashboard-content');
    if (this.checked) {
      dashboardContent.classList.add('visible');
    } else {
      dashboardContent.classList.remove('visible');
    }
  });
</script>


